{
  "name": "voice-assistant-js",
  "version": "0.1.0",
  "main": "dist/src/handler/AutoDetectHandler.js",
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.18",
    "actions-on-google": "^1.0.9",
    "alexa-sdk": "^1.0.9",
    "express": "^4.15.2",
    "jest": "^20.0.0",
    "standard": "^10.0.2",
    "ts-jest": "^20.0.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2",
    "typescript-standard": "^0.3.30"
  },
  "dependencies": {
    "di-typescript": "^1.1.3",
    "reflect-metadata": "^0.1.10"
  },
  "peerDependencies": {
    "actions-on-google": "^1.0.9",
    "alexa-sdk": "^1.0.9"
  },
  "scripts": {
    "cs": "standard --pretty src/**/*.ts",
    "test": "jest",
    "lint": "tslint --fix -p .",
    "coverage": "jest --coverage",
    "build": "NODE_ENV=production rm -rf dist/* && tsc && rm -rf dist/node_modules/aws-sdk"
