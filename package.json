    "cache": false,
    "verbose": true,
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "mapCoverage": true,
    "testMatch": [
      "**/tests/**/*Spec.ts"
    ]
  }
